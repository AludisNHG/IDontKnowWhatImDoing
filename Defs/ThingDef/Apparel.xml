<Defs>
 <ThingDef ParentName="ApparelMakeableBase">
    <defName>pphhyy_Brachyura_BoneArmor</defName>
    <label>Bone Trophies</label>
    <description>Brachyura enforcers preferred method to keep human slaves in line. Made from the skulls of Weak humans.</description>
    <graphicData>
      <texPath>Apparels/Brachyura_BoneArmor/BoneTrophies</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <techLevel>Neolithic</techLevel>
    <costStuffCount>20</costStuffCount>
    <costList>
      <Skull>5</Skull>
    </costList>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <tradeability>Sellable</tradeability>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
      <WorkToMake>2500</WorkToMake>
      <Mass>1</Mass>
      <StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.01</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.01</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.0</EquipDelay>
    </statBases>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <apparel>
    <useWornGraphicMask>true</useWornGraphicMask>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparels/Brachyura_BoneArmor/BoneTrophies</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <!-- <li>Neolithic</li> -->
      </tags>
      <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
    </apparel>
    <equippedStatOffsets>
      <SuppressionPower>0.5</SuppressionPower>
    </equippedStatOffsets>
    <recipeMaker>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
        <li>CraftingSpot</li>
        <li>Brachyura_ArmorBench</li>
      </recipeUsers>
    </recipeMaker>
  </ThingDef>

