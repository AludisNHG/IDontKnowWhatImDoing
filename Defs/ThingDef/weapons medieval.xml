<Defs>

  <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
    <defName>pphhyy_Brachyura_ChitinTrident</defName>
    <label>Chitin Trident.</label>
    <techLevel>Neolithic</techLevel>
    <description>A Medieval Brachyura Chitin Trident.</description>
    <graphicData>
      <texPath>Weapons/Brachyura_ChitinTrident</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.5</drawSize>
    </graphicData>
    <uiIconScale>1.2</uiIconScale>
    <weaponTags>
      <li>MedievalMeleeBasic</li>
    </weaponTags>
    <weaponClasses>
      <li>Neolithic</li>
    </weaponClasses>
    <costList>
      <Brachyura_Chitin>30</Brachyura_Chitin>
      <WoodLog>30</WoodLog>
    </costList>
    <statBases>
      <WorkToMake>6000</WorkToMake>
      <Mass>1</Mass>
    </statBases>
    <equippedAngleOffset>-65</equippedAngleOffset>
    <burnableByRecipe>True</burnableByRecipe>
    <tools>
      <li>
        <label>handle</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>point</label>
        <capacities>
          <li>Stab</li>
        </capacities>
        <power>16</power>
        <cooldownTime>2</cooldownTime>
        <armorPenetration>0.2</armorPenetration>
          <extraMeleeDamages>
              <li>
                <def>Stab</def>
                <amount>6</amount>
                <chance>1</chance>
              </li>
              <li>
                <def>Stab</def>
                <amount>6</amount>
                <chance>1</chance>
              </li>
          </extraMeleeDamages>
      </li>
    </tools>
    <recipeMaker>
    <researchPrerequisite>Brachyura_MedievalWeapons</researchPrerequisite>
      <recipeUsers>
        <li>CraftingSpot</li>
        <li>Brachyura_WeaponBench</li>
      </recipeUsers>
    </recipeMaker>
    <comps>
      <li Class="CompOversizedWeapon.CompProperties_OversizedWeapon">
        <verticalFlipNorth>false</verticalFlipNorth>
        <angleAdjustmentNorth>-160</angleAdjustmentNorth>
        <angleAdjustmentEast></angleAdjustmentEast>
        <angleAdjustmentWest></angleAdjustmentWest>
        <angleAdjustmentSouth></angleAdjustmentSouth>

        <northOffset>(0.0,-1,0.1)</northOffset>  <!--  a b(disapears) c(disapears) d(up by alot) e(up/down) -->
        <southOffset>(-0.0,0,0.0)</southOffset>
        <westOffset>(0.0,0,0.0)</westOffset>
        <eastOffset>(-0.0,0,0.0)</eastOffset>
      </li>
    </comps>
    <modExtensions>
      <li MayRequire="OskarPotocki.VanillaFactionsExpanded.Core" Class="VFECore.ThingDefExtension">
        <usableWithShields>false</usableWithShields>
      </li>
    </modExtensions>
  </ThingDef>

