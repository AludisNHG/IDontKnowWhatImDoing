<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
    <defName>pphhyy_Brachyura_ChitinSword</defName>
    <label>chitin Sword</label>
    <techLevel>Neolithic</techLevel>
    <description>A primitive Brachyura Chitin Sword.</description>
    <graphicData>
      <texPath>Weapons/Brachyura_ChitinSword</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <uiIconScale>1.2</uiIconScale>
    <weaponTags>
      <!-- <li>SkyRim_Forsworn_Melee_basic</li> -->
      <li>NeolithicMeleeBasic</li>
    </weaponTags>
    <weaponClasses>
      <li>Neolithic</li>
    </weaponClasses>
    <costList>
      <Brachyura_Chitin>20</Brachyura_Chitin>
    </costList>
    <statBases>
      <WorkToMake>4000</WorkToMake>
      <Mass>1</Mass>
    </statBases>
    <equippedAngleOffset>-65</equippedAngleOffset>
    <burnableByRecipe>True</burnableByRecipe>
    <tools>
      <li>
        <label>handle</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>blade</label>
        <capacities>
          <li>Cut</li>
        </capacities>
        <power>13</power>
        <cooldownTime>1.5</cooldownTime>
      </li>
      <li>
        <label>point</label>
        <capacities>
          <li>Stab</li>
        </capacities>
        <power>10</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
    <recipeMaker>
    <researchPrerequisite>Brachyura_PrimitiveWeapons</researchPrerequisite>
      <recipeUsers>
        <li>CraftingSpot</li>
      </recipeUsers>
    </recipeMaker>
    <comps>
      <li Class="CompOversizedWeapon.CompProperties_OversizedWeapon">
        <verticalFlipNorth>false</verticalFlipNorth>
        <angleAdjustmentNorth>-160</angleAdjustmentNorth>
        <angleAdjustmentEast></angleAdjustmentEast>
        <angleAdjustmentWest></angleAdjustmentWest>
        <angleAdjustmentSouth></angleAdjustmentSouth>

        <northOffset>(0.0,-1,0.1)</northOffset>  
        <southOffset>(-0.0,0,0.0)</southOffset>
        <westOffset>(0.0,0,0.0)</westOffset>
        <eastOffset>(-0.0,0,0.0)</eastOffset>
      </li>
    </comps>
    <modExtensions>
      <li MayRequire="OskarPotocki.VanillaFactionsExpanded.Core" Class="VFECore.ThingDefExtension">
        <usableWithShields>true</usableWithShields>
      </li>
    </modExtensions>
  </ThingDef>

