<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>Brachyura_MakeMaleBrachyuraPrecursor</defName>
    <label>make male Brahcyura mash</label>
    <description>Make male Brahcyura mash from Biomass and Eggs.</description>
    <jobString>Making Male Brachyura Eggmash.</jobString>
    <workSpeedStat>DrugCookingSpeed</workSpeedStat>
    <workSkill>Cooking</workSkill>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>1000</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Biomass</li>
          </thingDefs>
        </filter>
        <count>200</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>EggBrachyuraCoastalUnfertilized</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Biomass</li>
        <li>EggBrachyuraCoastalUnfertilized</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Brachyura_MaleBrachyuraPrecursor>1</Brachyura_MaleBrachyuraPrecursor>
    </products>
	<researchPrerequisite>Brachyura_Breeding_I</researchPrerequisite>
  </RecipeDef>

